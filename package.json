{
  "name": "ts-api-template",
  "version": "0.1.0",
  "private": true,
  "main": "server.ts",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build":"npm run prisma:generate:postgres && tsc",
    "start": "node dist/server.js",
    "prisma": "prisma",
    "prisma:generate:postgres":"prisma generate --schema=prisma/schema.postgres.prisma",
    "prisma:push:postgres":"prisma db push --schema=prisma/schema.postgres.prisma",
    "prisma:migrate:deploy":"prisma migrate deploy --schema=prisma/schema.postgres.prisma",
    "db:seed":"ts-node prisma/prisma.seed.ts",
    "test":"jest"
  },
  "type": "commonjs",
  "dependencies": {
    "@prisma/client": "^5.10.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "mongoose": "^7.5.0",
    "pino": "^8.12.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.5.6",
    "jest": "^30.2.0",
    "prisma": "^5.10.1",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
