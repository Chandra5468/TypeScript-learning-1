generator client {
  provider = "prisma-client-js"
  output   = "./generated/postgres" // create prisma/generated/postgres
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  schemas = ["audit", "test", "real"]
}

model Example {
  id        Int      @id @default(autoincrement())
  name      String
  createdAt DateTime @default(now())
}
